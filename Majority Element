class Solution {
    public int majorityElement(int[] nums) {
        int cnt = 0;
        int el = 0;
        for(int i=0;i<nums.length;i++){
            if(cnt==0){
                el = nums[i];
            }if(el==nums[i]){
                cnt+=1;
            }else{
                cnt-=1;
            }
        }
        return el;
    }
}
