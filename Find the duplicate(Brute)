public class first {
    public static void main(String[] args) {
        int[] arr={2,5,9,6,9,3,8,9,7,1};
        Arrays.sort(arr);
        for (int i=0;i< arr.length;i++){
            if (arr[i]==arr[i+1]){
                System.out.println(arr[i]);
                return;
            }
        }
    }
}
